//WidgetStore Widget Manifest
//대소문자 구별 지켜서 작성 바랍니다. 
//주석에 항목의 디폴트값은 default:".."로 적히며 항목의 값이 여러개인 경우 {"..",".."} 형태로 표기하였음
<?xml version="1.0" encoding="utf-8"?>
<WidgetStore>
    <required>
        <manifest_version>1</manifest_version>
        <widget>
            <name>SimpleClock</name>
            <version>1.0</version>
            <root>./index.html</root>
        </widget>
        <isGit>no</isGit>
        <git>
            <id>gooddaumi@naver.com</id>
            <repository>WidgetStore</repository>
            <tag>1.41</tag>                               default : ""
            <branch>master</branch>                       default : "master"
        </git>
     </required>

    <recommanded>
        <support_resolution>yes</support_resolution>      default:"no", {"yes","no"}
        <resolution method="ratio"/>                      default:"unsupported" ,{"unsupported","ratio","free"}
            <ratio>
                <first ratio="1:1" res="300,300">
                <second ratio="3:4" res="300,400">
                <third ratio="16:9" res="400,300">
            </ratio>
            <free>
                <minWidthRatio="300">
                <minHeightRatio="300">
                <maxWidthRatio="900">
                <maxHeightRatio="900">
            </free>           

        <icon-path="./image/icon/title.jpg">
    </recommanded>

    <optional>
        


    </optional>


   
</WidgetStore>